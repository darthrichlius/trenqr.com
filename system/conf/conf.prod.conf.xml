        <default_lang value="fr" />  
        <available_langs>
            <!-- [DEPUIS 10-07-16] -->
<!--            <lang value="en" /> 
            <lang value="fr" />  -->
            <lang name="lang" value="fr" />  
            <lang name="lang" value="en" /> 
            <!-- 
                [DEPUIS 10-07-16] 
                Si on a pas de traduction spécifique, on considérera us comme "en" via des copoer coller ou autres ajustements.
            -->
            <lang name="lang" value="us" /> 
        </available_langs>
        <!-- [DEPUIS 03-08-16] -->
        <available_lang_aliases>
            <!--
                Googlebot va crawler le site en anglais et tuer notre SEO.
                Tant qu'on a pas des URLs HREFLANG ou qu'on a pas traduit intégralement le site, on reste sur 'fr'.
                On aurait aussi pu retirer des LANG à <available_langs> pour arriver au même résultat.
            -->
            <lang_alias id="en" value="fr" />
            <lang_alias id="us" value="fr" />
        </available_lang_aliases>
        <url_params_required>
            <url_param id="page" value="simple" />
            <url_param id="urqid" value="simple" />
